{
  "manifest_version": 2,
  "name": "sasha_varit_bjuk",
  "version": "1.1",

  "description": "Calculate proteins, fat, carbs and calories for order on site 'https://www.edatomsk.ru/' (updated on orders chagnes)",
  "icons": {
    "96": "icons/fat-96.png"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "{8BAA29CA-3F0A-4C2D-9597-1F302D75AC55}",
      "strict_min_version": "61.0"
    }
  },

  "content_scripts": [
    {
      "matches": ["https://www.edatomsk.ru/*"],
      "js": ["main.js"],
      "css": ["bjuk.css"]
    }
  ]
}
